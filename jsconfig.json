{
	"compilerOptions": {
		"target": "ES2022",
		"lib":["DOM","DOM.Iterable","ES2022"],
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"jsx":"react-jsx",
		"strict":true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"outDir": "dist",
	},
	"include": ["src"],
	"exclude":["node_modules","dist","**/*.test.*"]
}